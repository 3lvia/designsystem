{
  "name": "elvia-designsystem",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "concurrently \"npm run start\" \"cd style/elvis && gulp watch\"",
    "build": "cd style/elvis && npm install && cd ../.. && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress": "cypress open",
    "commitizen-unix": "git cz --hook || true",
    "commitizen-windows": "winpty git cz --hook || true"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "./scripts/commitizen.sh",
      "pre-commit": "./scripts/pre-commit.sh",
      "pre-push": "./scripts/pre-push.sh"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/forms": "^9.0.7",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@angular/router": "^9.0.7",
    "@elvia/elvis": "^0.0.10",
    "classlist.js": "^1.1.20150312",
    "css-vars-ponyfill": "^2.3.0",
    "gulp": "^4.0.2",
    "prismjs": "^1.20.0",
    "rxjs": "^6.5.5",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "tslib": "^1.11.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "^9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/language-service": "^9.0.7",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.37",
    "codelyzer": "^5.2.2",
    "commitizen": "^4.1.2",
    "concurrently": "^5.2.0",
    "cypress": "^4.4.1",
    "cz-conventional-changelog": "^3.2.0",
    "del": "^5.1.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
