{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "outDir": "./dist/",
    "jsx": "react",
    "plugins": [
      {
        "transform": "typescript-plugin-styled-components",
        "type": "config"
      },
      {
        "name": "typescript-plugin-css-modules",
        "options": {
          "allowUnknownClassnames": true
        }
      }
    ]
  }
}
