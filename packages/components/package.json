{
  "name": "@elvia/elvis-components",
  "version": "0.0.1",
  "private": "true",
  "description": "WIP",
  "license": "ISC",
  "author": "",
  "main": "server.js",
  "scripts": {
    "start-dev": "concurrently \"webpack-dev-server --config ./webpack.config.js --mode development\" \"webpack --watch --mode development\" \"yarn run gulp:watch\"",
    "build": "yarn run build:component && yarn run build:loader",
    "build:components": "webpack --mode=production  --config ./webpack.config.js && gulp --gulpfile build/gulpfile.js",
    "build:components:dev": "webpack --mode=\"development\" --config ./webpack.config.js && gulp --gulpfile build/gulpfile.js",
    "build:components:watch": "webpack --mode=production  --config ./webpack.config.js --watch",
    "gulp": "gulp --gulpfile build/gulpfile.js",
    "gulp:watch": "gulp --gulpfile build/gulpfile.js cleanup && gulp --gulpfile build/gulpfile.js watch",
    "build:react": "rollup -c build/rollup.config.js"
  },
  "devDependencies": {
    "@elvia/elvis-component-wrapper": "*",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "del": "^6.0.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-header": "^2.0.9",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-tap": "^2.0.0",
    "merge-stream": "^2.0.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0"
  }
}
